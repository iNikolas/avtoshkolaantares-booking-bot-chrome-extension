{
  "manifest_version": 3,
  "name": "Antares Booking Bot",
  "version": "1.0.0",
  "description": "A simple React app as a Chrome extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "storage"],
  "host_permissions": ["https://www.booking.avtoshkolaantares.com/*"],
  "icons": {
    "32": "icon-32.png"
  }
}
